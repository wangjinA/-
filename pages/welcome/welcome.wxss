.logo-info{
  text-align: center;
}
.logo{
  width: 400rpx;
  height: 300rpx;
}
.logo-info .app-name{
  margin-bottom: 10rpx;
  color: #333;
  font-size: 34rpx;
}
.logo-info .tips{
  color: #999;
  font-size: 28rpx;
}
.user-list{
  display: flex;
  justify-content: space-around;
  margin-top: 100rpx;
}
.user-item{
  width: 200rpx;
  text-align: center;
}
.user-logo{
  /* margin: 0 20rpx; */
  font-size: 28rpx;
}
.user-logo image{
  width: 200rpx;
  height: 200rpx;
}
.welcome-hoel image{
  transform: scale(1.2);
}
.user-select-tips{
  color: #999;
  font-size: 28rpx;
  margin-top: 80rpx;
  text-align: center;
}

.login-btn{
  margin: 0;
  background-color: transparent;
  border: none;
  line-height: 1;
}
.login-btn::after{
  border: none;
}
.login-btn::before{
  border: none;
}